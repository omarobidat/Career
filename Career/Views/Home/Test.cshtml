@model IEnumerable<Career.Data.Skill>

@{
    ViewData["Title"] = "Test";
}

<h2>Test</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>

<div>
    @Html.ListBox("SportsList", new SelectList(Model, "SkillID", "SkillName"))
</div>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SkillName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SkillName)
            </td>

        </tr>
}
    </tbody>
</table>
